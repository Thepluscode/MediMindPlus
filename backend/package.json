    "knex": "^3.1.0",
    "ml-matrix": "^6.12.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "shap": "^1.1.3",
    "stripe": "^19.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "twilio": "^5.10.5",
    "typeorm": "^0.3.27",
    "typescript": "^5.8.3",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
